# stream-sdk SDK Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Security Configuration
# The SDK supports loading security credentials from environment variables
# with the prefix: STREAMSDK_
#
# Security environment variables:
STREAMSDK_JWT_BEARER=your_jwtbearer_here

# Debug Configuration
# Enable HTTP request/response logging for debugging
STREAMSDK_DEBUG=true
